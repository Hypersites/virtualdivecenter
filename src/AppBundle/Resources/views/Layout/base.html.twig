<!-- topbar ends -->
<div class="ch-container">
    <div class="row">

        <!-- left menu starts -->
        <div class="col-sm-2 col-lg-2">
            <div class="sidebar-nav">
                {% include 'AppBundle:Layout:sidebar.html.twig' %}
            </div>
        </div>

        <div id="content" class="col-lg-10 col-sm-10">
            <!-- content starts -->
            <div>
                <ul class="breadcrumb">
                  {% for item in breadcrumbs %}
                      <li>{{ item|trans }}</li>
                  {% endfor %}
                </ul>
            </div>
            <div class=" row">
                {% include 'AppBundle:Layout:info.html.twig' %}
            </div>

            <div class="row">
                <div class="box col-md-12">
                    <div class="box-inner">
                        {% block body %}{% endblock %}
                </div>
            </div>
        </div>
    </div>
    <hr>
    <footer class="row">
        <p class="col-md-9 col-sm-9 col-xs-12 copyright">&copy;{% trans %} Developed By {% endtrans %} <a href="http://www.hypersites.com.br">Hypersites</a>
        </p>
    </footer>

</div>
<script src="{{ asset('bundles/app/dist/js/bootstrap.min.js')}}"></script>

<!-- library for cookie management -->
<script src="{{ asset('bundles/app/js/jquery.cookie.js')}}"></script>
